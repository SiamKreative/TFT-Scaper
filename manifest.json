{
  "name": "ThaiFood.today-Scraper",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Custom scraper build for ThaiFood.today",
  "homepage_url": "https://thaifood.today/",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}